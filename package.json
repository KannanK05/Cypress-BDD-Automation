  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "babel-loader": "^9.1.3",
    "cypress": "^13.1.0",
    "cypress-cucumber-preprocessor": "latest",
    "cypress-parallel": "^0.13.0",
    "mochawesome": "^7.1.3",
